class Solution {
    public boolean canSplitArray(List<Integer> nums, int m) {
        int n = nums.size();
        if(n <= 2 || m == 1)return true;
        int sum=0;
        for(int i = 1; i<n;i++){
            sum = nums.get(i) + nums.get(i-1);
            if(sum >= m){
                return true;
            }
        }
        return false;
    }
}
